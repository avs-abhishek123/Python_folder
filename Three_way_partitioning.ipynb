      "source": [
        "n = 5\n",
        "arr = [1, 2, 3, 3, 4]\n",
        "a = 1\n",
        "b = 2\n",
        "l,m,h = 0,0,n-1\n",
        "while m<=h:\n",
        "    if arr[m]<a: \n",
        "        arr[l],arr[m]=arr[m],arr[l] \n",
        "        l+=1 \n",
        "        m+=1 \n",
        "    elif arr[m]>b:\n",
        "        arr[m],arr[h] = arr[h],arr[m]\n",
        "        h-=1\n",
        "    else:\n",
        "        m+=1\n",
        "print(arr)"
      ],
      "execution_count": 3,
      "outputs": [
        {
          "output_type": "stream",
          "text": [
            "[1, 2, 3, 4, 3]\n"
          ],
          "name": "stdout"
        }
